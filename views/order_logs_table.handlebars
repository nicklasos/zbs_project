<link rel="stylesheet" type="text/css" href="/styles/order_logs.css">
<table class="table" id="order_logs">
    <thead>
    <tr>
        <th>id</th>
        <th>status</th>
        <th>message</th>
        <th>date</th>
        <th><button type="button" class="btn btn-default btn-sm" id="page_reload">Reload</button></th>
    </tr>
    </thead>
    <tbody>
    {{#each allOrderLogs}}
        <tr class="{{status}}">
            <td>{{id}}</td>
            <td>{{status}}</td>
            <td>{{message}}</td>
            <td colspan="2">{{dateFormat updated_at "Y-MM-DD HH:mm:ss"}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
<script src="/js/order_logs.js"></script>